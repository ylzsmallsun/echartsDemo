{
  "presets": ["react", "env", "es2015"],
  "env": {
    "development": {
        "plugins": [
            ["transform-object-rest-spread", "react-transform", {
                "transforms": [
                    {
                        "transform": "react-transform-hmr",
                        
                        "imports": ["react"],
                        
                        "locals": ["module"]
                    }
                ]
            }]
        ]
    }
  }
}
